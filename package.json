{
  "private": true,
  "scripts": {
    "sass:build": "sass src/assets/css/src:public/assets/css/dist --no-source-map --style=expanded",
    "sass:watch": "sass --watch src/assets/css/src:public/assets/css/dist --no-source-map --style=expanded",
    "build:themes": "node ./scripts/build-themes.js",
    "postcss:build": "postcss \"public/assets/css/dist/**/*.css\" --dir public/assets/css/dist",
    "postcss:watch": "postcss \"public/assets/css/dist/**/*.css\" --dir public/assets/css/dist --watch",
    "dev:css": "pnpm run sass:watch & pnpm run postcss:watch",
    "dev:theleague-watch": "node ./scripts/watch-theleague-assets.mjs",
    "dev": "pnpm run dev:css & pnpm run dev:theleague-watch & astro dev",
    "start": "astro dev",
    "build:css": "pnpm run sass:build && pnpm run postcss:build",
    "bundle:tools": "vite build --config scripts/league-tools.vite.config.mjs",
    "build": "pnpm run build:css && astro build && pnpm run bundle:tools",
    "sync:theleague": "node ./scripts/sync-theleague-assets.mjs",
    "predev": "pnpm run sync:theleague",
    "prebuild": "pnpm run sync:theleague",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/vercel": "^8.0.4",
    "astro": "^5.1.1",
    "chokidar": "^3.6.0",
    "react": "18.3.1",
    "typescript": "^5.9.3"
  }
  ,
  "devDependencies": {
    "sass": "^1.76.0",
    "postcss": "^8.4.34",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1"
  }
}
